<div class="password card">
    <div class="card-header">
        <h2>Create a Password</h2>
    </div>
    <form id="new-password-form" class="card-body">
        
        <label class="form-label" for="password-title">Title</label>
        <input type="text" name="password-title" class="form-input" />
        
        <label class="form-label" for="password-email">Email</label>
        <input type="text" name="password-email" class="form-input" />

        <label class="form-label" for="password-username">Username</label>
        <input type="text" name="password-username" class="form-input" />

        <label class="form-label" for="password-password">Password</label>
        <input type="text" name="password-password" class="form-input" />

        <label class="form-label" for="password-website_url">URL</label>
        <input type="text" name="password-website_url" class="form-input" />

        <button type="submit" class="btn">Create</button>
    </form>
</div>

<script src="/javascript/new.js"></script>