<div class="post">
    <article class="card">
        <div class="card-header flex-row justify-space-between">
            <h2>{{title}}</h2>
        </div>
        <ul class="card-body">
            <li>Website Title: {{title}}</li>
            <li>Email: {{email}}</li>
            <li>Username: {{username}}</li>
            <li>Password: {{password}}</li>
            <li>URL: {{website_url}}</li>
        </ul>
    </article>
</div>
  